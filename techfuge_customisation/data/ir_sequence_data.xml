<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="seq_event_reference_number" model="ir.sequence">
            <field name="name">Event Reference Number</field>
            <field name="code">event.reference.number</field>
            <field name="prefix">EVENT</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_event_attendee_registration_number" model="ir.sequence">
            <field name="name">Attendee Registration Number</field>
            <field name="code">attendee.registration.number</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>